<div class="udex-rating-indicator {{classNames}}"
	role="slider"
	aria-roledescription="{{_ariaRoleDescription}}"
	aria-valuemin="0"
	aria-valuenow="{{value}}"
	aria-valuemax="{{max}}"
	aria-valuetext="{{value}} of {{max}}"
	aria-orientation="horizontal"
	aria-disabled="{{_ariaDisabled}}"
	aria-readonly="{{ariaReadonly}}"
	aria-description="{{_ariaDescription}}"
	tabindex="{{effectiveTabIndex}}"
	@focusin="{{_onfocusin}}"
	@focusout="{{_onfocusout}}"
	@click="{{_onclick}}"
	@keydown="{{_onkeydown}}"
	title="{{ratingTooltip}}"
	aria-label="{{_ariaLabel}}"
>
	<ul class="udex-rating-indicator__list"
		aria-hidden="true"
		>
		{{#each _stars}}
			{{#if this.selected}}
				<li data-ui5-value="{{this.index}}" class="udex-rating-indicator__item udex-rating-indicator__item-sel">
					<ui5-icon data-ui5-value="{{this.index}}" name="favorite"></ui5-icon>
				</li>
			{{else if this.halfStar}}
				<li class="udex-rating-indicator__item udex-rating-indicator__item-half">
					<ui5-icon data-ui5-value="{{this.index}}" name="unfavorite"></ui5-icon>
					<div class="udex-rating-indicator__half-icon-wrapper">
						<ui5-icon data-ui5-value="{{this.index}}" name="favorite" class="udex-rating-indicator__half-icon"></ui5-icon>
					</div>
				</li>
			{{else if ../readonly}}
				<li class="udex-rating-indicator__item udex-rating-indicator__item-unsel">
					<ui5-icon data-ui5-value="{{this.index}}" name="favorite"></ui5-icon>
				</li>
			{{else if ../disabled}}
				<li class="udex-rating-indicator__item udex-rating-indicator__item-unsel">
					<ui5-icon data-ui5-value="{{this.index}}" name="unfavorite"></ui5-icon>
				</li>
			{{else}}
				<li data-ui5-value="{{this.index}}" class="udex-rating-indicator__item udex-rating-indicator__item-unsel">
					<ui5-icon data-ui5-value="{{this.index}}" name="unfavorite"></ui5-icon>
				</li>
			{{/if}}
		{{/each}}
	</ul>
  {{#if totalCount}}
    <div class="udex-rating-indicator__total">({{totalCount}})</div>
  {{/if}}
</div>