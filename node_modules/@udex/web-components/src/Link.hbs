<ui5-link id="udex-link" class="{{linkClass}}">
    {{#if startIcon}}
        <ui5-icon class="udex-link__icon udex-link__icon--icon-start" name="{{icon}}"></ui5-icon>
    {{/if}}
    <span class="udex-link__content">
        <slot>link</slot>
    </span>
    {{#if endIcon}}
        <ui5-icon class="udex-link__icon udex-link__icon--icon-end" name="{{icon}}"></ui5-icon>
    {{/if}}
</ui5-link>