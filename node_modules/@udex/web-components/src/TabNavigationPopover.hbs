<ui5-popover
    id="{{_id}}-overflow-popover"
    horizontal-align="Right"
	placement-type="Bottom"
    aria-orientation="vertical"
    hide-arrow
    _hideHeader
>
    <div
        class="tab-navigation-popover"
        @click="{{handleOverflowItemClick}}"
        @keydown="{{handleFocusPopover}}"
        role="tablist"
    >
        {{#each _overflowItems}}
            {{tabInOverflow}}
        {{/each}}
    </div>
</ui5-popover>
