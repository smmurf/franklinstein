<ui5-button 
  class="udex-button {{sizeClass}}"
  ?disabled="{{disabled}}"
  icon="{{icon}}"
  ?icon-end="{{iconEnd}}"
  tooltip="{{tooltip}}"
  design="{{design}}"
  ?toggled="{{toggled}}"
  accessible-name="{{accessibleName}}"
  accessible-name-ref="{{accessibleNameRef}}"
  @click="{{onButtonClick}}"
>
  {{#if renderIconOnlyButton}}
    <slot></slot>
  {{/if}}
</ui5-button>
