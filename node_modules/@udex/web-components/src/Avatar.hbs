<ui5-avatar
    class="{{udexAvatarClass}}"
    ?disabled="{{disabled}}"
    initials="{{initials}}"
    ?interactive="{{interactive}}"
    accessible-name="{{accessibleName}}"
    color-scheme="{{getUI5AvatarColorScheme}}"
    fallback-icon="{{fallbackIcon}}"
    icon="{{icon}}"
    shape="{{shape}}"
    size="{{size}}"
>
    {{#if imageSlot}}
        <slot></slot>
    {{/if}}
    {{#if badgeSlot}}
        <slot name="badge" slot="badge"></slot>
    {{/if}}
</ui5-avatar>
