<div class="{{tagClasses}}" @click="{{tagClickHandler}}" tabindex="0" ?role="{{accessibleRole}}" aria-selected="{{_selected}}">
        {{#if startIcon}}
            <ui5-icon class="udex-tag__icon udex-tag__icon udex-tag__icon--start" name="{{icon}}" @click="{{iconClickHandler}}"></ui5-icon>
        {{/if}}
        {{text}}
        {{#if endIcon}}
            <ui5-icon class="udex-tag__icon udex-tag__icon udex-tag__icon--end" name="{{icon}}" @click="{{iconClickHandler}}"></ui5-icon>
        {{/if}}
</div>