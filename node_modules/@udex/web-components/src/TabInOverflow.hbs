{{#if hasNotHref}}
    <button
        id="{{this._id}}"
        class="udex-tab-overflow__button"
        role="tab"
    >
        {{#if hasIcon}}
            <span class="udex-tab-overflow__button-icon">
                <ui5-icon
                    name="{{icon}}"
                ></ui5-icon>
                {{#if showAttentionBadge}}
                    <span class="udex-tab-overflow__button-badge"></span>
                {{/if}}
            </span>
        {{/if}}
        {{text}}
        {{#if hasNotIcon}}
            <span class="udex-tab-overflow__button-badge"></span>
        {{/if}}
    </button>
{{else}}
    <a
        href="{{href}}"
        target="{{this.target}}"
        class="udex-tab-overflow__button"
        role="tab"
    >
        {{#if hasIcon}}
            <span class="udex-tab-overflow__button-icon">
                <ui5-icon
                    name="{{icon}}"
                ></ui5-icon>
                {{#if showAttentionBadge}}
                    <span class="udex-tab-overflow__button-badge"></span>
                {{/if}}
            </span>
        {{/if}}
        {{text}}
        {{#if hasNotIcon}}
            <span class="udex-tab-overflow__button-badge"></span>
        {{/if}}
    </a>
{{/if}}
